<Window x:Class="MusicListSorter.AddMultipleRecordsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add Multiple Records" Height="80" Width="280">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <Button x:Name="ImportFromExcelButton" Content="Import from Excel" Grid.Row="0" Width="150" Height="30" Margin="10"
                HorizontalAlignment="Center" VerticalAlignment="Center" Click="ImportFromExcel_Click"/>
    </Grid>
</Window>
